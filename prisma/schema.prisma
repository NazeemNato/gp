generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       Int    @id @default(autoincrement())
  fullName String
  email String @unique
  username String @unique
  password String
  post Post[]
}

model Post {
  id       Int    @id @default(autoincrement())
  author  Users @relation(fields: [authorId], references: [id])
  authorId Int
  title String
  body String
}